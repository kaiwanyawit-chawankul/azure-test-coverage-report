  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --collectCoverage --ci --reporters=default --reporters=jest-junit"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
